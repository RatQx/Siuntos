@model AutoNuoma.Models.Uzsakymas

@{
    ViewBag.Title = "Naujas užsakymas";
}

<h2>Naujas užsakymas</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <fieldset>
        <legend>Užsakymo informacija</legend>
        <div class="form-horizontal">
            

            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Pildyti duomenis" class="btn btn-success" />
                </div>
            </div>
        </div>
    </fieldset>
}

<div>
    @Html.ActionLink("Nutraukti operacija >>", "Index", "Home", new { area = "" }, new { @class = "btn btn-default" })
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
